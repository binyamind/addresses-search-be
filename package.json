{
  "name": "addresses-search-be",
  "version": "1.0.0",
  "description": "addresses-search-backend",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "tsc",
    "copy": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./dist",
    "build": "npm run clean && npm run compile && npm run copy",
    "start": "npm run build && node dist/index.js",
    "start:dev": "npx nodemon",
    "elastic:start": "docker-compose -f elastic-compose.yaml up -d",
    "elastic:stop": "docker-compose -f elastic-compose.yaml down",
    "elastic:restart": "docker-compose -f elastic-compose.yaml restart",
    "elastic:build": "docker-compose -f elastic-compose.yaml build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "@elastic/elasticsearch": "^8.15.0",
    "@types/elasticsearch": "^5.0.43",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "csv-elasticsearch": "^1.1.3",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "express-openid-connect": "^2.17.1",
    "express-validator": "^7.2.0",
    "http-status-codes": "^2.3.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "copyfiles": "^2.4.1",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
